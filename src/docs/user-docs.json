{
  "/users/notifications": {
    "get": {
      "description": "Get notifications",
      "responses": {
        "500": {
          "description": "Common error",
          "content": {}
        },
        "200": {
          "description": "Success",
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "notification": {
                    "type": "object",
                    "$ref": "#/components/schemas/Notification"
                  }
                }
              }
            }
          }
        }
      }
    }
  },

  "/users/{userId}": {
    "get": {
      "parameters": [
        {
          "name": "userId",
          "in": "path",
          "example": "61ed31eedacddd4d88b1daf6",
          "schema": {
            "type": "string"
          },
          "required": true
        }
      ],
      "description": "Get one user",
      "responses": {
        "500": {
          "description": "Common error",
          "content": {}
        },
        "200": {
          "description": "Success",
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "User": {
                    "type": "object",
                    "$ref": "#/components/schemas/User"
                  }
                }
              }
            }
          }
        }
      }
    }
  },
  "/{userId}/follow": {
    "post": {
      "security": [
        {
          "bearerAuth": []
        }
      ],
      "parameters": [
        {
          "name": "userId",
          "in": "path",
          "example": "61ed31eedacddd4d88b1daf6",
          "schema": {
            "type": "string"
          },
          "required": true
        }
      ],

      "description": "Follow",
      "responses": {
        "500": {
          "description": "Common error",
          "content": {}
        },
        "401": {
          "description": "Authorization failed",
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "message": {
                    "type": "string",
                    "example": "Unauthtorized"
                  }
                }
              }
            }
          }
        },
        "200": {
          "description": "Success",
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "post": {
                    "type": "object",
                    "$ref": "#/components/schemas/User"
                  }
                }
              }
            }
          }
        }
      }
    }
  }
}
