  "/posts/{_id}": {
    "get": {
      "parameters": [
        {
          "name": "_id",
          "in": "path",
          "example": "61ed31eedacddd4d88b1daf6",
          "schema": {
            "type": "string"
          },
          "required": true
        }
      ],
      "description": "Get post with id",
      "responses": {
        "500": {
          "description": "Common error",
          "content": {}
        },
        "200": {
          "description": "Success",
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "post": {
                    "type": "object",
                    "$ref": "#/components/schemas/Post"
                  }
                }
              }
            }
          }
        }
      }
    },
    "put": {
      "security": [
        {
          "bearerAuth": []
        }
      ],
      "requestBody": {
        "content": {
          "application/json": {
            "schema": {
              "type": "object",
              "properties": {
                "title": {
                  "type": "string",
                  "example": "Post 1"
                },
                "body": {
                  "type": "string",
                  "example": "body 1"
                },
                "views": {
                  "type": "number",
                  "example": 20
                },
                "thumbnailUrl": {
                  "type": "string",
                  "example": "www.photo.com"
                },
                "createdAt": {
                  "type": "string",
                  "format": "date-time",
                  "example": "2022-01-23T10:46:06.831Z"
                },
                "updatedAt": {
                  "type": "string",
                  "format": "date-time",
                  "example": "2022-01-23T10:46:06.831Z"
                },
                "_id": {
                  "type": "string",
                  "format": "uuid",
                  "example": "61ed31eedacddd4d88b1daf6"
                }
              }
            }
          }
        }
      },
      "parameters": [
        {
          "name": "_id",
          "in": "path",
          "example": "61ed31eedacddd4d88b1daf6",
          "schema": {
            "type": "string"
          },
          "required": true
        }
      ],
      "description": "Change post",
      "responses": {
        "500": {
          "description": "Common error",
          "content": {}
        },
        "401": {
          "description": "Authorization failed",
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "message": {
                    "type": "string",
                    "example": "Unauthtorized"
                  }
                }
              }
            }
          }
        },
        "200": {
          "description": "Success",
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "title": {
                    "type": "string",
                    "example": "Post 1"
                  },
                  "body": {
                    "type": "string",
                    "example": "body 1"
                  },
                  "views": {
                    "type": "number",
                    "example": 20
                  },
                  "thumbnailUrl": {
                    "type": "string",
                    "example": "www.photo.com"
                  },
                  "createdAt": {
                    "type": "string",
                    "format": "date-time",
                    "example": "2022-01-23T10:46:06.831Z"
                  },
                  "updatedAt": {
                    "type": "string",
                    "format": "date-time",
                    "example": "2022-01-23T10:46:06.831Z"
                  },
                  "_id": {
                    "type": "string",
                    "format": "uuid",
                    "example": "61ed31eedacddd4d88b1daf6"
                  }
                },
                "required": ["title", "body"]
              }
            }
          }
        }
      }
    },
    "delete": {
      "security": [
        {
          "bearerAuth": []
        }
      ],
      "parameters": [
        {
          "name": "_id",
          "in": "path",
          "example": "61ed31eedacddd4d88b1daf6",
